    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <VBox layoutX="139.0" layoutY="43.0" prefHeight="322.0" prefWidth="361.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="73.0" text="Identifiant">
